{
  "name": "apprentistagedjango",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/ApprentiStageDjango",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/ApprentiStageDjango",
        "commands": [
          {
            "command": "python3 manage.py runserver"
          }
        ]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "packages/ApprentiStageDjango/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:8000"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ApprentiStageDjango/**/*.{ts,js}"]
      }
    }
  },
  "implicitDependencies": []
}
